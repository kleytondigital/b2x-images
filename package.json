{
    "name": "b2x-gestor-images",
    "version": "1.0.0",
    "description": "Gestor profissional de imagens com MinIO",
    "main": "server/index.js",
    "scripts": {
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "server": "nodemon server/index.js",
        "client": "cd client && npm start",
        "build": "cd client && npm run build",
        "start": "node server/index.js",
        "install-all": "npm install && cd client && npm install",
        "build-all": "npm run build && npm run start",
        "deploy": "npm run install-all && npm run build && npm start"
    },
    "dependencies": {
        "express": "^4.18.2",
        "cors": "^2.8.5",
        "helmet": "^7.1.0",
        "multer": "^1.4.5-lts.1",
        "minio": "^7.1.3",
        "bcryptjs": "^2.4.3",
        "jsonwebtoken": "^9.0.2",
        "dotenv": "^16.3.1",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "nodemon": "^3.0.2",
        "concurrently": "^8.2.2"
    },
    "keywords": ["gestor", "imagens", "minio", "upload"],
    "author": "B2X",
    "license": "MIT"
}